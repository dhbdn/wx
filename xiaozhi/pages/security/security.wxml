<!--pages/security/security.wxml-->
<view class='container'>
    <view class='col changePwd row' bindtap='setLoginPwd'>
      <image class='icon' src='../../images/register/pwd.png'></image>
      <view class='text'>修改登录密码</view>
      <image class='arrow' src='../../images/account/5.png'></image>
    </view>
    <view class='col changePwd row' bindtap='setPayPwd'>
      <image class='icon' src='../../images/register/pwd.png'></image>
      <view class='text'>设置支付密码</view>
      <image class='arrow' src='../../images/account/5.png'></image>
    </view>
</view>
