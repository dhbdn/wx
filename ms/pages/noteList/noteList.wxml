<!--pages/noteList/noteList.wxml-->
<view class='container'>
  <block wx:for="{{noteList}}" wx:key="index" >
    <view class='content row' data-index="{{index}}" catchtap='ontip'>
    <view class='item'>{{list[index]}}</view>
      <text class='time item'>{{noteList[index].createTime}} > </text>
    </view>
  </block>
</view>
