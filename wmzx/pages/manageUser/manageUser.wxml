<!--pages/manageUser/manageUser.wxml-->
<view class='container'>
  <view class='content'>
    <view class='item row'>
      <block wx:for="{{list}}" wx:key="{{index}}" >
        <view catchtap='onTap' data-index="{{index}}" class='column'>
          <image src='{{list[index].img}}'></image>
          <text class='txt'>{{list[index].txt}}</text>
        </view>
      </block>
    </view>
  </view>
</view>